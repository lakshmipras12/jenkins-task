pipeline {
    agent any
    stages {
        stage('git clone') {
            steps {
                git branch: 'master',
                url: 'https://github.com/lakshmipras12/jenkins-task.git'
            }
        }

        stage ('build') {
            steps {
                sh 'cat jenkinfile.sh'
            }
        }

    }
    post {
        success {
            emailext subject: 'deployed successfully',
            body: 'jenkins task executed successfully',
            to: 'menthemklpn61@gmail.com',
            attachLog: true
        }
    }

}
